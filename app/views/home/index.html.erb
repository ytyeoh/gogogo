<div class="contain">
  <div class="row">
    <%= form_tag listings_path, method: :get do %>
      <div class="col-md-12">
        <div class="form-group">
          <%= text_field_tag :query, params[:query], class: 'form-control', id: 'city', placeholder: "Where you want to rent?",  autocomplete: "off" %>
